# 80 "/opt/topspin3.7.0/exp/stan/nmr/lists/pp/Delay.incl"
;$Id: Delay.incl,v 1.14.2.1 2013/08/30 09:43:33 ber Exp $
# 6 "/home/waudbyc/pp_702/19F_cest.cw" 2


define list<frequency> F19sat = <$FQ1LIST>
# 10 "/home/waudbyc/pp_702/19F_cest.cw"
"d11=30m"
"d12=20u"

; for baseopt
"acqt0=-p1*2/3.1416"

# 1 "mc_line 16 file /home/waudbyc/pp_702/19F_cest.cw exp. def. part of mc cmd. before ze"
; dimension 2D; AQ_mode  (F1) QF
define delay MCWRK
define delay MCREST
"MCREST = d1 - d1"
"MCWRK = 0.500000*d1"

    dccorr
# 16 "/home/waudbyc/pp_702/19F_cest.cw"
1 ze 
# 1 "mc_line 16 file /home/waudbyc/pp_702/19F_cest.cw exp. def. of mc cmd. after ze"
LBLAV, MCWRK
# 17 "/home/waudbyc/pp_702/19F_cest.cw"
# 1 "mc_line 17 file /home/waudbyc/pp_702/19F_cest.cw exp. start label for mc cmd."
2 MCWRK 
LBLF1, MCWRK
  MCREST
# 18 "/home/waudbyc/pp_702/19F_cest.cw"
# 19 "/home/waudbyc/pp_702/19F_cest.cw"
  ; CEST period
  4u pl8:f1 
  4u F19sat:f1
  d18 cw:f1 ph11
  1u do:f1 

  ; purge
  4u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0
  p16:gp1
  d16 pl1:f1 fq=0:f1
  4u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7

# 34 "/home/waudbyc/pp_702/19F_cest.cw"
 p1 ph1
 4u
 p1 ph2
 4u
 p1 ph3
;------------------------------------

  go=2 ph31 
# 1 "mc_line 42 file /home/waudbyc/pp_702/19F_cest.cw exp. mc cmd. in line"
  MCWRK  wr #0 if #0 zd F19sat.inc 
  lo to LBLF1 times td1
  MCWRK rf #0
  lo to LBLAV times tdav
# 44 "/home/waudbyc/pp_702/19F_cest.cw"

exit 
  
# 48 "/home/waudbyc/pp_702/19F_cest.cw"
ph1 =0
ph2 =2 0
ph3 =0 0 2 2 1 1 3 3
ph11=0 
ph31=0 2 2 0 1 3 3 1


;p16: homospoil/gradient pulse                       [0.5 msec]
;d1 : relaxation delay (excluding saturation time)
;d11: delay for disk I/O                             [30 msec]
;d12: delay for power switching                      [20 usec]
;d16: delay for homospoil/gradient recovery
;d18: saturation time
;pl8: f1 channel - power level for CEST saturation
;ns: 1 * n
;ds: 4
# 66 "/home/waudbyc/pp_702/19F_cest.cw"
;for z-only gradients:
;gpz1: 41%

;use gradient files:   
;gpnam1: SMSQ10.100

